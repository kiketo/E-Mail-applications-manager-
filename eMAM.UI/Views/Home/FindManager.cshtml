@model List<ManagerViewModel>
@{
    ViewData["Title"] = "FindAdmin";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<p>Contact a Manager:</p>
@foreach (var managerInfo in @Model)
{
<address>
    <strong>User:</strong>  @managerInfo.UserName<br />
    <strong>Email:</strong> <a href="mailto:@managerInfo.Email">@managerInfo.Email</a>
    
</address>
   
}