        FROM "Emails" AS e0
        INNER JOIN "Statuses" AS "e.Status0" ON e0."StatusId" = "e.Status0"."Id"
        INNER JOIN "Senders" AS "e.Sender0" ON e0."SenderId" = "e.Sender0"."Id"
        ORDER BY e0."Id"
        LIMIT @__p_1 OFFSET @__p_0
    ) AS t
) AS t0 ON "e.Attachments"."EmailId" = t0."Id"
ORDER BY t0."Id"
